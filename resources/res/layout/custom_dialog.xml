<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent" app:layoutDescription="@xml/custom_dialog_scene">
    <TableRow android:background="@color/Background" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="18sp" android:textStyle="bold" android:textColor="@color/text_white" android:id="@+id/texttitle" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="5dp" android:layout_marginTop="20dp" android:layout_marginBottom="20dp" android:text="LICENCE KEY - (Not Available)" android:textAllCaps="true"/>
    </TableRow>
    <TableRow android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:id="@+id/MainKey" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="20dp" android:text="report"/>
    </TableRow>
    <TableRow android:layout_width="match_parent" android:layout_height="wrap_content">
        <EditText android:id="@+id/edt_comment" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="20dp" android:hint="Enter Passcode to Run App" android:inputType="textMultiLine"/>
    </TableRow>
    <TableRow android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="20dp">
        <Button android:id="@+id/btnCopy" android:background="#f8b400" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="8dp" android:text="Copy" android:layout_weight="1"/>
        <Button android:id="@+id/buttonCancel" android:background="#f80000" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="8dp" android:text="cancel" android:layout_weight="1"/>
        <Button android:id="@+id/buttonSubmit" android:background="#00d05a" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="8dp" android:text="submitt" android:layout_weight="1"/>
    </TableRow>
</LinearLayout>
